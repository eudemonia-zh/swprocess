{
    "workflow": "frequency-domain",
    "pre-processing": {
        "trim": {
            "apply": true,
            "start_time": 0,
            "end_time": 0.75
        },
        "mute": {
            "apply": false,
            "type": "interactive",
            "window kwargs": {}
        },
        "pad": {
            "apply": true,
            "df": 1
        }
    },
    "processing": {
        "type": "phaseshift",
        "fmin": 5,
        "fmax": 100,
        "vmin": 100,
        "vmax": 400,
        "nvel": 200,
        "vspace": "linear",
        "fdbf-specific": {
            "weighting": "sqrt",
            "steering": "cylindrical"
        }
    },
    "signal-to-noise": {
        "perform": true,
        "noise": {
            "start": -0.5,
            "end": 0.0
        },
        "signal": {
            "start": 0.0,
            "end": 0.5
        },
        "pad": {
            "apply": true,
            "df": 1
        }
    }
}